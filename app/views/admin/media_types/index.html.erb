<p id="notice"><%= notice %></p>

<h1>Media Types</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% for media_type in @media_types %>
    <tr>
      <td><%= media_type.name %></td>
      <td><%= link_to "Show", [:admin, media_type] %></td>
      <td><%= link_to "Edit", edit_admin_media_type_path(media_type) %></td>
      <td><%= link_to "Destroy", [:admin, media_type], :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Media Type", new_admin_media_type_path %></p>
